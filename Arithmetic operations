#include<stdio.h>
int main() {
    int a,b;
     printf("Enter first number\n");
     scanf("%d\n",&a);
     printf("Enter second number\n");
     scanf("%d\n",&b);
    int m,c,o,p;
     m=a*b;
     c=a+b;
     o=a-b;
     p=a/b;
    printf("The sum of two numbers is %d\n",c);
    printf("The Multiplication of two numbers is %d\n",m);
    printf("The difference of two numbers is %d\n",o);
    printf("The divison of two numbers is %d\n",p);
    return 0;
}
